{
  "name": "@zen-web-components/calendar",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A headless calendar component for LitElement",
  "author": "Travis True <travis.true08@gmail.com> (http://www.travistrue.io)",
  "main": "src/calendar.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zen-web-components/calendar.git"
  },
  "engines": {
    "node": ">=8.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "install:clear": "rm -rf node_modules",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook -o _site -s storybook-static",
    "site:build": "npm run storybook:build",
    "test": "echo $CHROME_BIN; ls /usr/bin/google-chrome; karma start --single-run --coverage",
    "test:watch": "karma start  --coverage --auto-watch"
  },
  "keywords": [
    "calendar",
    "component",
    "lit",
    "viewer",
    "zen",
    "zensen"
  ],
  "devDependencies": {
    "@open-wc/demoing-storybook": "^0.3.0",
    "@open-wc/testing": "^0.11.4",
    "@open-wc/testing-karma": "^1.1.1",
    "@zen-web-components/eslint-config": "^1.0.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "deepmerge": "^4.2.2",
    "eslint": "^7.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^4.2.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.1",
    "mocha": "^6.1.3",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
